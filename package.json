{
  "name": "kot-kit",
  "version": "1.0.0",
  "description": "Just another UI kit",
  "private": true,
  "main": "dist/entries/lib.js",
  "module": "dist/entries/lib-es.js",
  "types": "dist/entries/lib.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build-dev": "cross-env NODE_ENV=development rollup -c && mv dist/lib.js dist/entries/lib.js && mv dist/lib-es.js dist/entries/lib-es.js",
    "lint": "eslint -c config/linters/eslint.json lib/ && tslint -c config/linters/tslint.json --project tsconfig.json --exclude \"**/showcase/**/*.*\"",
    "showcase": "cd showcase && npm start",
    "docs": "node_modules/typedoc/bin/typedoc --externalPattern \"**/node_modules/**/*.*\" --exclude \"**/showcase/**/*.*\" --target es6 --theme minimal --ignoreCompilerErrors --excludeExternals --mode modules --includeDeclarations global.d.ts --tsconfig tsconfig.json --out dist/docs lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bungu/kot-kit.git"
  },
  "author": "Artem Nechunaev <nechunaev@tutu.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bungu/kot-kit/issues"
  },
  "homepage": "https://github.com/bungu/kot-kit#readme",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/deepmerge": "^1.3.3",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.38",
    "@types/react-input-mask": "^0.7.7",
    "@types/react-modal": "^3.1.1",
    "@types/react-transition-group": "^2.0.7",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-proto-to-assign": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.0.5",
    "deepmerge": "^2.0.1",
    "eslint": "^4.7.2",
    "eslint-plugin-react": "^7.3.0",
    "jss": "^9.3.3",
    "jss-preset-default": "^4.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-input-mask": "^1.1.2",
    "react-jss": "^8.1.0",
    "react-modal": "^3.1.11",
    "rollup": "^0.52.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.8.4",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-visualizer": "^0.3.1",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2",
    "uglify-es": "^3.2.0"
  },
  "dependencies": {
    "lodash-es": ">=4",
    "marked": "^0.3.12",
    "react-day-picker": "^7.0.7",
    "react-popper": "^0.8.2",
    "react-transition-group": "^2.2.1"
  },
  "peerDependencies": {
    "classnames": ">=2",
    "deepmerge": ">=2",
    "jss": ">=9",
    "jss-preset-default": ">=4",
    "react": ">=16",
    "react-dom": ">=16",
    "react-input-mask": ">=1",
    "react-jss": ">=8",
    "react-modal": ">=3"
  }
}
